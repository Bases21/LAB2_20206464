<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lab2">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".RoutersActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".SwitchesActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".AddRouterActivity"
            android:exported="false"
            android:parentActivityName=".RoutersActivity" />

        <activity
            android:name=".EditRouterActivity"
            android:exported="false"
            android:parentActivityName=".RoutersActivity" />

        <activity
            android:name=".AddSwitchActivity"
            android:exported="false"
            android:parentActivityName=".SwitchesActivity" />

        <activity
            android:name=".EditSwitchActivity"
            android:exported="false"
            android:parentActivityName=".SwitchesActivity" />
        <activity
            android:name=".AccessPointsActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".AddAccessPointActivity"
            android:exported="false"
            android:parentActivityName=".AccessPointsActivity" />

        <activity
            android:name=".EditAccessPointActivity"
            android:exported="false"
            android:parentActivityName=".AccessPointsActivity" />
        <activity
            android:name=".ReporteActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />

    </application>

</manifest>